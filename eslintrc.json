{
    "extends": "airbnb",
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "parserOptions": {
        "ecmaFeatures": { "jsx": true },
        "sourceType": "module"
    },
    "rules": {
        "spaced-comment": "off",
        "no-console": "off",
        "no-unused-vars": "off",
        "no-multi-spaces": "off",
        "import/no-extraneous-dependencies": ["error", {"devDependencies": true}]
    }
}
